#: cryptmount.c:126
msgid ""
"usage: cryptmount [OPTION [target ...]]\n"
"\n"
"  available options are as follows:\n"
"\n"
"    -h | --help\n"
"    -a | --all\n"
"    -c | --change-password <target>\n"
"    -k | --key-managers\n"
"    -l | --list\n"
"    -S | --status\n"
"    -m | --mount <target>\n"
"    -u | --unmount <target>\n"
"    --generate-key <key-size> <target>\n"
"    --reuse-key <src-target> <dst-target>\n"
"    --prepare <target>\n"
"    --release <target>\n"
"    --config-fd <num>\n"
"    --passwd-fd <num>\n"
"    --swapon <target>\n"
"    --swapoff <target>\n"
"    --version\n"
"\n"
"  please report bugs to <cryptmount@rwpenney.uk>\n"
msgstr ""

